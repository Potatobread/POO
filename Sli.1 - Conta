package Slide;

public class Conta {
	private String numero;
	private double saldo;
	
	void credito(double valor) {
		saldo += valor;
	}
	void debito(double valor) {
		saldo -= valor;
	}
	String getNumero() {
		return numero;
	}
	double getSaldo() {
		return saldo;
	}
	
	//Construtor
	Conta(String n){
		numero = n;
		saldo = 0;
	}
}
